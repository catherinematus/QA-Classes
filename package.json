{
  "name": "mocha-ts-unit-testing",
  "version": "1.0.0",
  "description": "Mocha-Typescript Unit Testing Framework",
  "main": "spec.ts",
  "scripts": {
    "intro-demo": "ts-node ./intro/demo/index.ts",
    "intro-home": "ts-node ./intro/home/index.ts",
    "unit-demo-mocha": "mocha --config='./unit/demo/.mocharc.json'",
    "unit-demo-jest": "jest --config='./unit/demo/jest.config.js'",
    "unit-demo-mocha-coverage": "nyc npm run unit-demo-mocha",
    "unit-home-mocha": "rm -rf allure-results allure-report && mocha --config='./unit/home/.mocharc.json'",
    "unit-home-report": "allure generate allure-results --clean -o allure-report && allure open",
    "unit-home-jest": "jest --config='./unit/home/jest.config.js'",
    "api-demo": "jest --config='./api/demo/jest.config.js'",
    "api-home": "jest --config='./api/home/jest.config.js'",
    "api-logging": "mocha --config='./axios-winston/.mocharc.json'",
    "selenium-demo": "ts-node ./selenium/demo/index.ts",
    "selenium-home": "mocha --config='./selenium/home/.mocharc.json'",
    "selenium-cucumber-test": "cucumber-js --config=./selenium-cucumber/configs/cucumber.json",
    "selenium-cucumber-report": "ts-node ./selenium-cucumber/configs/html.report.config.ts",
    "patterns": "mocha --config='./patterns/configs/.mocharc.json'",
    "wdio-cucumber": "wdio ./wdio/config/wdio.conf.ts",
    "wdio-cucumber-report": "allure generate wdio/allure-results --clean -o wdio/allure-report && allure open ./wdio/allure-report",
    "playwright": "playwright test --config=./playwright/configs/playwright.config.ts",
    "playwright-report": "playwright show-report playwright/assets/reports",
    "playwright-allure-report": "allure generate playwright/assets/allure-results --clean -o playwright/assets/allure-report && allure open ./playwright/assets/allure-report",
    "cy:e2e:open": "cypress open --config-file=./cy-e2e/cypress.config.ts",
    "cy:e2e:run": "cypress run --browser chrome --config-file=./cy-e2e/cypress.config.ts",
    "cy:e2e:allure:report": "allure generate cy-e2e/cypress/assets/allure-results --clean -o cy-e2e/cypress/assets/allure-report && allure open ./cy-e2e/cypress/assets/allure-report",
    "cy:integration:open": "cypress open --config-file=./cy-integration/cypress.config.ts",
    "cy:integration:run": "cypress run --config-file=./cy-integration/cypress.config.ts",
    "cy:integration:allure:report": "allure generate cy-integration/cypress/assets/allure-results --clean -o cy-integration/cypress/assets/allure-report && allure open ./cy-integration/cypress/assets/allure-report"
  },
  "keywords": [
    "typescript",
    "api",
    "selenium",
    "cucumber",
    "webdriverio",
    "playwright",
    "cypress"
  ],
  "author": "VladSelikh",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^8.5.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@playwright/test": "^1.25.1",
    "@shelex/cypress-allure-plugin": "^2.31.0",
    "@types/chai": "^4.3.1",
    "@types/jest": "^28.1.6",
    "@types/mocha": "^9.1.1",
    "@types/mochawesome": "^6.2.1",
    "@types/node": "^18.6.3",
    "@types/randomstring": "^1.1.8",
    "@types/selenium-webdriver": "^4.1.2",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@wdio/allure-reporter": "^7.23.0",
    "@wdio/cli": "^7.23.0",
    "@wdio/cucumber-framework": "^7.23.0",
    "@wdio/local-runner": "^7.23.0",
    "@wdio/spec-reporter": "^7.23.0",
    "ajv": "^8.11.0",
    "allure-commandline": "^2.18.1",
    "allure-playwright": "^2.0.0-beta.19",
    "axios": "^0.27.2",
    "axios-logger": "^2.6.1",
    "chai": "^4.3.6",
    "chromedriver": "^104.0.0",
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^10.7.0",
    "cypress-mochawesome-reporter": "^3.2.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jest": "^28.1.3",
    "jest-allure": "^0.1.3",
    "jest-html-reporter": "^3.6.0",
    "jest-jasmine2": "^28.1.3",
    "log4js": "^6.6.1",
    "mocha": "^10.0.0",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0",
    "randomstring": "^1.2.2",
    "selenium-webdriver": "^4.4.0",
    "superagent": "^8.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wdio-chromedriver-service": "^7.3.2",
    "wdio-geckodriver-service": "^3.0.6",
    "winston": "^3.8.2"
  }
}
